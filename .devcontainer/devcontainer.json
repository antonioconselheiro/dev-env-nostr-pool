{
  "name": "just a Strfry in a Devcontainer",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "strfry-dev",
	"workspaceFolder": "/strfry-dev",
  "postAttachCommand": "bash -c 'tail -f /log/log.log && bash'",
	"customizations": {
		"vscode": {
			"extensions": [
        "ms-azuretools.vscode-docker",
        "Endormi.2077-theme",
        "eamodio.gitlens"
      ]
		}
	}
}
